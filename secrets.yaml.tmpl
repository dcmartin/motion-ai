logger-binary_sensor: error
logger-camera: error
logger-components: info
logger-counter: info
logger-core: error
logger-loader: error
logger-mqtt-sensor: error
logger-recorder-util: error
logger-recorder: error
logger-sensor: error
logger-setup: error
logger-statistics-sensor: fatal
logger-template-sensor: error
logger-template-binary_sensor: error
